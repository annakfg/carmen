real rho, rhoE;
real th=pi/3.;
real phi=pi/4.;
real X = x*cos(th)-y*sin(th);
real Y = (x*sin(th)+y*cos(th))*cos(phi)-z*sin(phi);
real Z = (x*sin(th)+y*cos(th))*sin(phi)+z*cos(phi);
real a = 1./3.;
real b = 1.;
real c = 3.;
real r = sqrt((X*X)/(a*a)+(Y*Y)/(b*b)+(Z*Z)/(c*c));
real R = 0.6;

if (r<R)
{
rho=0.125;
rhoE=0.25;
}
else
{
rho=1.;
rhoE=2.5;
}
